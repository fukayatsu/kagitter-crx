{
   "manifest_version": 2,
   "name": "Kagitter",
   "version": "0.0.2",
   "options_page": "options.html",
   "background": {
      "scripts": [
         "js/background.js"
      ]
   },
   "content_scripts": [{
      "matches": ["https://twitter.com/*"],
      "js": [
         "js/jquery-1.9.1.min.js",
         "js/content_script.js"
      ]
   }],
   "browser_action": {
      "default_icon": "img/icon128.png",
      "default_title": "Kagitter",
      "default_popup": "popup.html"
   },
   "icons": {
      "128": "img/icon128.png",
      "48": "img/icon48.png"
   }
}
